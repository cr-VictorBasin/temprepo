pipeline {
    agent any

    stages {
        stage("Using curl example") {
            steps {
                script {
                    final String url = "http://ds-manager-dev.cr-devops-internal-dev.com:8000/query/tasks/task_id/102973656/"

                    final String response = sh(script: "curl -s $url | jq '.query_result[] | {task_id, entry_date, status}'", returnStdout: true).trim()

                    echo response
                }
            }
        }
    }
}