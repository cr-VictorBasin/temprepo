pipeline {
    agent any

    parameters {
        string(defaultValue: "TEST", description: "Which env to deploy in?", name: "deployEnv")
        choice(choices: ["EU-WEST-2A", "EU-WEST-2B", "EU-WEST-2C"], description: "Which AZ to deploy TO?", name: "deployAZ")
        booleanParam(defaultValue: false, description: "Enable service?", name: "myBoolean")
    }

    stages {
        stage ("Demo") {
            steps {
                echo "What is custom deploy name?: ${deployEnv} \n"
                echo "Select which AZ to deploy to: ${params.deployAZ}"
                echo "Confirm the deployment: ${params.myBoolean}"
            }
        }
        
    }

}